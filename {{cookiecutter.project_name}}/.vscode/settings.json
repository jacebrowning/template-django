{
    "files.exclude": {
      ".cache/": true,
      ".coverage": true,
      ".envrc": true,
      ".pytest_cache": true,
      ".venv/": true,
      "*.egg-info/": true,
      "*.log": true,
      "**/__pycache__/": true,
      "htmlcov/": true,
      "staticfiles/": true
    },
    "python.defaultInterpreterPath": ".venv/bin/python",
    "pylint.args": ["--rcfile=.pylint.ini"],
    "editor.formatOnSave": true,
    "cSpell.words": [
      "asgi",
      "asgiref",
      "bugsnag",
      "buildpacks",
      "circleci",
      "cleandata",
      "collectstatic",
      "favicons",
      "gunicorn",
      "httpx",
      "loaddata",
      "lpignore",
      "mrkdwn",
      "ngrok",
      "onfocus",
      "pgbouncer",
      "postdeploy",
      "pythonunbuffered",
      "runserver",
      "screenfull",
      "urlunparse",
      "venv",
      "wsgi",
    ]
  }
